{
    "name": "rampall/laravel-console-dusk",
    "description": "Laravel Console Dusk allows the usage of Laravel Dusk in Laravel/Laravel Zero artisan commands.",
    "keywords": [
        "console",
        "command-line",
        "php",
        "cli",
        "laravel-zero",
        "laravel",
        "Laravel Dusk",
        "artisan",
        "symfony"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rampall/laravel-console-dusk/issues",
        "source": "https://github.com/rampall/laravel-console-dusk"
    },
    "authors": [
        {
            "name": "Ramesh Pallikara",
            "email": "rameshpallikara@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "rampall/dusk": "^0.0.2",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "rampall/laravel-console-task": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "RamPall\\LaravelConsoleDusk\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RamPall\\LaravelConsoleDusk\\LaravelConsoleDuskServiceProvider"
            ]
        }
    },
    "suggest": {
        "phpunit/phpunit": "Required to perform browser test assertions."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}